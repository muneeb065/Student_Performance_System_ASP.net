@{
    ViewBag.Title = "LoginForm";
}

<h2>LoginForm</h2>
<form method="post" action="StudentPersonalInfo">
    <table align="center">
        <tr>
            <td> <label> Name:</label></td>
            <td> <input type="text" placeholder="Muneeb" name="name" id="name" required /></td>
        </tr>
        <tr>
            <td> <label> Email-id:</label></td>
            <td> <input type="email" placeholder="muneeb@gmail.com" name="mail" id="mail" required /></td>
        </tr>
        <tr>
            <td> <label> Password:</label></td>
            <td> <input type="password" placeholder="Asp#2022" name="password" id="password" required /></td>
        </tr>
    </table>
</form>